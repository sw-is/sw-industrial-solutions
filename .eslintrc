{  "extends": ["standard", "plugin:react/recommended"],  "plugins": [    "react"  ],  "rules": {    "no-return-assign": 0,    "react/react-in-jsx-scope": 0,    "semi": [2, "always"],    "spaced-comment": 0,    "no-tabs": 0,    "object-curly-spacing": 0,    "indent": [2, "tab"],    "react/prop-types": 0,    "no-mixed-spaces-and-tabs": 0,    "callback-return": 0,    "array-callback-return": 0,    "no-empty": 0,    "no-lone-blocks": 0,    "eol-last": 0  }}